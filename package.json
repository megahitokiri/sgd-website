{
  "name": "sgd-website",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon ./bin/www",
    "build-css": "stylus public/stylesheets/styles.styl -o public/stylesheets/",
    "watch-css": "stylus public/stylesheets/styles.styl -o public/stylesheets/ -w",
    "clean": "rm -f public/stylesheets/*.css",
    "build": "npm run clean && npm run build-css",
    "watch": "npm run clean && npm run watch-css & nodemon ./bin/www -e styl,pug"
  },
  "dependencies": {
    "body-parser": "~1.15.2",
    "cookie-parser": "~1.4.3",
    "debug": "~2.2.0",
    "express": "~4.14.0",
    "helmet": "^3.5.0",
    "morgan": "~1.7.0",
    "pug": "~2.0.0-beta6",
    "require-dir": "^0.3.1",
    "serve-favicon": "~2.3.0",
    "stylus": "0.54.5",
    "vhost": "^3.0.2"
  },
  "devDependencies": {
    "browser-sync": "^2.18.8",
    "del": "^2.2.2",
    "gulp": "git://github.com/gulpjs/gulp.git#4.0",
    "gulp-autoprefixer": "^3.1.1",
    "gulp-cached": "^1.1.1",
    "gulp-concat": "^2.6.1",
    "gulp-copy": "^1.0.0",
    "gulp-csso": "^3.0.0",
    "gulp-debug": "^3.1.0",
    "gulp-filter": "^5.0.0",
    "gulp-imagemin": "^3.2.0",
    "gulp-jshint": "^2.0.4",
    "gulp-jslint": "^1.0.10",
    "gulp-load-plugins": "^1.5.0",
    "gulp-nodemon": "^2.2.1",
    "gulp-pug": "^3.3.0",
    "gulp-remember": "^0.3.1",
    "gulp-sourcemaps": "^2.5.0",
    "gulp-stylus": "^2.6.0",
    "gulp-uglify": "^2.1.2",
    "jshint": "^2.9.4",
    "main-bower-files": "^2.13.1",
    "nodemon": "^1.11.0",
    "through2": "^2.0.3",
    "yargs": "^7.0.2"
  }
}
