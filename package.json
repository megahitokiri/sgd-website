{
  "name": "sgd-website",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "build-css": "stylus public/stylesheets/styles.styl -o public/stylesheets/",
    "watch-css": "stylus public/stylesheets/styles.styl -o public/stylesheets/ -w",
    "clean": "rm -f public/stylesheets/css/*.css",
    "build": "npm run clean && npm run build-css",
    "watch": "npm run clean && npm run watch-css & nodemon ./bin/www -e styl,pug"
  },
  "dependencies": {
    "body-parser": "~1.15.2",
    "bootstrap": "^3.3.7",
    "cookie-parser": "~1.4.3",
    "debug": "~2.2.0",
    "express": "~4.14.0",
    "fontawesome": "^4.7.2",
    "helmet": "^3.5.0",
    "morgan": "~1.7.0",
    "nodemon": "^1.11.0",
    "pug": "~2.0.0-beta6",
    "serve-favicon": "~2.3.0",
    "stylus": "0.54.5"
  }
}
